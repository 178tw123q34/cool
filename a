[32m+[m[32mpublic class MainActivity extends AppCompatActivity[m
[32m+[m[32m        implements NavigationDrawerFragment.NavigationDrawerCallbacks {[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Fragment managing the behaviors, interactions and presentation of the navigation drawer.[m
[32m+[m[32m     */[m
[32m+[m[32m    private NavigationDrawerFragment mNavigationDrawerFragment;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Used to store the last screen title. For use in {@link #restoreActionBar()}.[m
[32m+[m[32m     */[m
[32m+[m[32m    private CharSequence mTitle;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_main);[m
[32m+[m
[32m+[m[32m        mNavigationDrawerFragment = (NavigationDrawerFragment)[m
[32m+[m[32m                getSupportFragmentManager().findFragmentById(R.id.navigation_drawer);[m
[32m+[m[32m        mTitle = getTitle();[m
[32m+[m
[32m+[m[32m        // Set up the drawer.[m
[32m+[m[32m        mNavigationDrawerFragment.setUp([m
[32m+[m[32m                R.id.navigation_drawer,[m
[32m+[m[32m                (DrawerLayout) findViewById(R.id.drawer_layout));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onNavigationDrawerItemSelected(int position) {[m
[32m+[m[32m        // update the main content by replacing fragments[m
[32m+[m[32m        FragmentManager fragmentManager = getSupportFragmentManager();[m
[32m+[m[32m        fragmentManager.beginTransaction()[m
[32m+[m[32m                .replace(R.id.container, PlaceholderFragment.newInstance(position + 1))[m
[32m+[m[32m                .commit();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void onSectionAttached(int number) {[m
[32m+[m[32m        switch (number) {[m
[32m+[m[32m            case 1:[m
[32m+[m[32m                mTitle = getString(R.string.title_section1);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 2:[m
[32m+[m[32m                mTitle = getString(R.string.title_section2);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 3:[m
[32m+[m[32m                mTitle = getString(R.string.title_section3);[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void restoreActionBar() {[m
[32m+[m[32m        ActionBar actionBar = getSupportActionBar();[m
[32m+[m[32m        actionBar.setNavigationMode(ActionBar.NAVIGATION_MODE_STANDARD);[m
[32m+[m[32m        actionBar.setDisplayShowTitleEnabled(true);[m
[32m+[m[32m        actionBar.setTitle(mTitle);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onCreateOptionsMenu(Menu menu) {[m
[32m+[m[32m        if (!mNavigationDrawerFragment.isDrawerOpen()) {[m
[32m+[m[32m            // Only show items in the action bar relevant to this screen[m
[32m+[m[32m            // if the drawer is not showing. Otherwise, let the drawer[m
[32m+[m[32m            // decide what to show in the action bar.[m
[32m+[m[32m            getMenuInflater().inflate(R.menu.main, menu);[m
[32m+[m[32m            restoreActionBar();[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return super.onCreateOptionsMenu(menu);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onOptionsItemSelected(MenuItem item) {[m
[32m+[m[32m        // Handle action bar item clicks here. The action bar will[m
[32m+[m[32m        // automatically handle clicks on the Home/Up button, so long[m
[32m+[m[32m        // as you specify a parent activity in AndroidManifest.xml.[m
[32m+[m[32m        int id = item.getItemId();[m
[32m+[m
[32m+[m[32m        //noinspection SimplifiableIfStatement[m
[32m+[m[32m        if (id == R.id.action_settings) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return super.onOptionsItemSelected(item);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * A placeholder fragment containing a simple view.[m
[32m+[m[32m     */[m
[32m+[m[32m    public static class PlaceholderFragment extends Fragment {[m
[32m+[m[32m        /**[m
[32m+[m[32m         * The fragment argument representing the section number for this[m
[32m+[m[32m         * fragment.[m
[32m+[m[32m         */[m
[32m+[m[32m        private static final String ARG_SECTION_NUMBER = "section_number";[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Returns a new instance of this fragment for the given section[m
[32m+[m[32m         * number.[m
[32m+[m[32m         */[m
[32m+[m[32m        public static PlaceholderFragment newInstance(int sectionNumber) {[m
[32m+[m[32m            PlaceholderFragment fragment = new PlaceholderFragment();[m
[32m+[m[32m            Bundle args = new Bundle();[m
[32m+[m[32m            args.putInt(ARG_SECTION_NUMBER, sectionNumber);[m
[32m+[m[32m            fragment.setArguments(args);[m
[32m+[m[32m            return fragment;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public PlaceholderFragment() {[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public View onCreateView(LayoutInflater inflater, ViewGroup container,[m
[32m+[m[32m                                 Bundle savedInstanceState) {[m
[32m+[m[32m            View rootView = inflater.inflate(R.layout.fragment_main, container, false);[m
[32m+[m[32m            return rootView;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public void onAttach(Activity activity) {[m
[32m+[m[32m            super.onAttach(activity);[m
[32m+[m[32m            ((MainActivity) activity).onSectionAttached([m
[32m+[m[32m                    getArguments().getInt(ARG_SECTION_NUMBER));[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
