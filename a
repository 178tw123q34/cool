[32m+[m[32mpackage com.example.administrator.myapplicationtwo;[m
[32m+[m
[32m+[m[32mimport android.support.v7.app.AppCompatActivity;[m
[32m+[m[32mimport android.app.Activity;[m
[32m+[m[32mimport android.support.v7.app.ActionBar;[m
[32m+[m[32mimport android.support.v4.app.Fragment;[m
[32m+[m[32mimport android.support.v4.app.ActionBarDrawerToggle;[m
[32m+[m[32mimport android.support.v4.view.GravityCompat;[m
[32m+[m[32mimport android.support.v4.widget.DrawerLayout;[m
[32m+[m[32mimport android.content.SharedPreferences;[m
[32m+[m[32mimport android.content.res.Configuration;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.preference.PreferenceManager;[m
[32m+[m[32mimport android.view.LayoutInflater;[m
[32m+[m[32mimport android.view.Menu;[m
[32m+[m[32mimport android.view.MenuInflater;[m
[32m+[m[32mimport android.view.MenuItem;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport android.view.ViewGroup;[m
[32m+[m[32mimport android.widget.AdapterView;[m
[32m+[m[32mimport android.widget.ArrayAdapter;[m
[32m+[m[32mimport android.widget.ListView;[m
[32m+[m[32mimport android.widget.Toast;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Fragment used for managing interactions for and presentation of a navigation drawer.[m
[32m+[m[32m * See the <a href="https://developer.android.com/design/patterns/navigation-drawer.html#Interaction">[m
[32m+[m[32m * design guidelines</a> for a complete explanation of the behaviors implemented here.[m
[32m+[m[32m */[m
[32m+[m[32mpublic class NavigationDrawerFragment extends Fragment {[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Remember the position of the selected item.[m
[32m+[m[32m     */[m
[32m+[m[32m    private static final String STATE_SELECTED_POSITION = "selected_navigation_drawer_position";[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Per the design guidelines, you should show the drawer on launch until the user manually[m
[32m+[m[32m     * expands it. This shared preference tracks this.[m
[32m+[m[32m     */[m
[32m+[m[32m    private static final String PREF_USER_LEARNED_DRAWER = "navigation_drawer_learned";[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * A pointer to the current callbacks instance (the Activity).[m
[32m+[m[32m     */[m
[32m+[m[32m    private NavigationDrawerCallbacks mCallbacks;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Helper component that ties the action bar to the navigation drawer.[m
[32m+[m[32m     */[m
[32m+[m[32m    private ActionBarDrawerToggle mDrawerToggle;[m
[32m+[m
[32m+[m[32m    private DrawerLayout mDrawerLayout;[m
[32m+[m[32m    private ListView mDrawerListView;[m
[32m+[m[32m    private View mFragmentContainerView;[m
[32m+[m
[32m+[m[32m    private int mCurrentSelectedPosition = 0;[m
[32m+[m[32m    private boolean mFromSavedInstanceState;[m
[32m+[m[32m    private boolean mUserLearnedDrawer;[m
[32m+[m
[32m+[m[32m    public NavigationDrawerFragment() {[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m
[32m+[m[32m        // Read in the flag indicating whether or not the user has demonstrated awareness of the[m
[32m+[m[32m        // drawer. See PREF_USER_LEARNED_DRAWER for details.[m
[32m+[m[32m        SharedPreferences sp = PreferenceManager.getDefaultSharedPreferences(getActivity());[m
[32m+[m[32m        mUserLearnedDrawer = sp.getBoolean(PREF_USER_LEARNED_DRAWER, false);[m
[32m+[m
[32m+[m[32m        if (savedInstanceState != null) {[m
[32m+[m[32m            mCurrentSelectedPosition = savedInstanceState.getInt(STATE_SELECTED_POSITION);[m
[32m+[m[32m            mFromSavedInstanceState = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Select either the default item (0) or the last selected item.[m
[32m+[m[32m        selectItem(mCurrentSelectedPosition);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onActivityCreated(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onActivityCreated(savedInstanceState);[m
[32m+[m[32m        // Indicate that this fragment would like to influence the set of actions in the action bar.[m
[32m+[m[32m        setHasOptionsMenu(true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public View onCreateView(LayoutInflater inflater, ViewGroup container,[m
[32m+[m[32m                             Bundle savedInstanceState) {[m
[32m+[m[32m        mDrawerListView = (ListView) inflater.inflate([m
[32m+[m[32m                R.layout.fragment_navigation_drawer, container, false);[m
[32m+[m[32m        mDrawerListView.setOnItemClickListener(new AdapterView.OnItemClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onItemClick(AdapterView<?> parent, View view, int position, long id) {[m
[32m+[m[32m                selectItem(position);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        mDrawerListView.setAdapter(new ArrayAdapter<String>([m
[32m+[m[32m                getActionBar().getThemedContext(),[m
[32m+[m[32m                android.R.layout.simple_list_item_activated_1,[m
[32m+[m[32m                android.R.id.text1,[m
[32m+[m[32m                new String[]{[m
[32m+[m[32m                        getString(R.string.title_section1),[m
[32m+[m[32m                        getString(R.string.title_section2),[m
[32m+[m[32m                        getString(R.string.title_section3),[m
[32m+[m[32m                }));[m
[32m+[m[32m        mDrawerListView.setItemChecked(mCurrentSelectedPosition, true);[m
[32m+[m[32m        return mDrawerListView;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public boolean isDrawerOpen() {[m
[32m+[m[32m        return mDrawerLayout != null && mDrawerLayout.isDrawerOpen(mFragmentContainerView);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Users of this fragment must call this method to set up the navigation drawer interactions.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param fragmentId   The android:id of this fragment in its activity's layout.[m
[32m+[m[32m     * @param drawerLayout The DrawerLayout containing this fragment's UI.[m
[32m+[m[32m     */[m
[32m+[m[32m    public void setUp(int fragmentId, DrawerLayout drawerLayout) {[m
[32m+[m[32m        mFragmentContainerView = getActivity().findViewById(fragmentId);[m
[32m+[m[32m        mDrawerLayout = drawerLayout;[m
[32m+[m
[32m+[m[32m        // set a custom shadow that overlays the main content when the drawer opens[m
[32m+[m[32m        mDrawerLayout.setDrawerShadow(R.drawable.drawer_shadow, GravityCompat.START);[m
[32m+[m[32m        // set up the drawer's list view with items and click listener[m
[32m+[m
[32m+[m[32m        ActionBar actionBar = getActionBar();[m
[32m+[m[32m        actionBar.setDisplayHomeAsUpEnabled(true);[m
[32m+[m[32m        actionBar.setHomeButtonEnabled(true);[m
[32m+[m
[32m+[m[32m        // ActionBarDrawerToggle ties together the the proper interactions[m
[32m+[m[32m        // between the navigation drawer and the action bar app icon.[m
[32m+[m[32m        mDrawerToggle = new ActionBarDrawerToggle([m
[32m+[m[32m                getActivity(),                    /* host Activity */[m
[32m+[m[32m                mDrawerLayout,                    /* DrawerLayout object */[m
[32m+[m[32m                R.drawable.ic_drawer,             /* nav drawer image to replace 'Up' caret */[m
[32m+[m[32m                R.string.navigation_drawer_open,  /* "open drawer" description for accessibility */[m
[32m+[m[32m                R.string.navigation_drawer_close  /* "close drawer" description for accessibility */[m
[32m+[m[32m        ) {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onDrawerClosed(View drawerView) {[m
[32m+[m[32m                super.onDrawerClosed(drawerView);[m
[32m+[m[32m                if (!isAdded()) {[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                getActivity().supportInvalidateOptionsMenu(); // calls onPrepareOptionsMenu()[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onDrawerOpened(View drawerView) {[m
[32m+[m[32m                super.onDrawerOpened(drawerView);[m
[32m+[m[32m                if (!isAdded()) {[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (!mUserLearnedDrawer) {[m
[32m+[m[32m                    // The user manually opened the drawer; store this flag to prevent auto-showing[m
[32m+[m[32m                    // the navigation drawer automatically in the future.[m
[32m+[m[32m                    mUserLearnedDrawer = true;[m
[32m+[m[32m                    SharedPreferences sp = PreferenceManager[m
[32m+[m[32m                            .getDefaultSharedPreferences(getActivity());[m
[32m+[m[32m                    sp.edit().putBoolean(PREF_USER_LEARNED_DRAWER, true).apply();[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                getActivity().supportInvalidateOptionsMenu(); // calls onPrepareOptionsMenu()[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        // If the user hasn't 'learned' about the drawer, open it to introduce them to the drawer,[m
[32m+[m[32m        // per the navigation drawer design guidelines.[m
[32m+[m[32m        if (!mUserLearnedDrawer && !mFromSavedInstanceState) {[m
[32m+[m[32m            mDrawerLayout.openDrawer(mFragmentContainerView);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Defer code dependent on restoration of previous instance state.[m
[32m+[m[32m        mDrawerLayout.post(new Runnable() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void run() {[m
[32m+[m[32m                mDrawerToggle.syncState();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        mDrawerLayout.setDrawerListener(mDrawerToggle);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void selectItem(int position) {[m
[32m+[m[32m        mCurrentSelectedPosition = position;[m
[32m+[m[32m        if (mDrawerListView != null) {[m
[32m+[m[32m            mDrawerListView.setItemChecked(position, true);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (mDrawerLayout != null) {[m
[32m+[m[32m            mDrawerLayout.closeDrawer(mFragmentContainerView);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (mCallbacks != null) {[m
[32m+[m[32m            mCallbacks.onNavigationDrawerItemSelected(position);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onAttach(Activity activity) {[m
[32m+[m[32m        super.onAttach(activity);[m
[32m+[m[32m        try {[m
[32m+[m[32m            mCallbacks = (NavigationDrawerCallbacks) activity;[m
[32m+[m[32m        } catch (ClassCastException e) {[m
[32m+[m[32m            throw new ClassCastException("Activity must implement NavigationDrawerCallbacks.");[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onDetach() {[m
[32m+[m[32m        super.onDetach();[m
[32m+[m[32m        mCallbacks = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onSaveInstanceState(Bundle outState) {[m
[32m+[m[32m        super.onSaveInstanceState(outState);[m
[32m+[m[32m        outState.putInt(STATE_SELECTED_POSITION, mCurrentSelectedPosition);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onConfigurationChanged(Configuration newConfig) {[m
[32m+[m[32m        super.onConfigurationChanged(newConfig);[m
[32m+[m[32m        // Forward the new configuration the drawer toggle component.[m
[32m+[m[32m        mDrawerToggle.onConfigurationChanged(newConfig);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onCreateOptionsMenu(Menu menu, MenuInflater inflater) {[m
[32m+[m[32m        // If the drawer is open, show the global app actions in the action bar. See also[m
[32m+[m[32m        // showGlobalContextActionBar, which controls the top-left area of the action bar.[m
[32m+[m[32m        if (mDrawerLayout != null && isDrawerOpen()) {[m
[32m+[m[32m            inflater.inflate(R.menu.global, menu);[m
[32m+[m[32m            showGlobalContextActionBar();[m
[32m+[m[32m        }[m
[32m+[m[32m        super.onCreateOptionsMenu(menu, inflater);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onOptionsItemSelected(MenuItem item) {[m
[32m+[m[32m        if (mDrawerToggle.onOptionsItemSelected(item)) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (item.getItemId() == R.id.action_example) {[m
[32m+[m[32m            Toast.makeText(getActivity(), "Example action.", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return super.onOptionsItemSelected(item);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Per the navigation drawer design guidelines, updates the action bar to show the global app[m
[32m+[m[32m     * 'context', rather than just what's in the current screen.[m
[32m+[m[32m     */[m
[32m+[m[32m    private void showGlobalContextActionBar() {[m
[32m+[m[32m        ActionBar actionBar = getActionBar();[m
[32m+[m[32m        actionBar.setDisplayShowTitleEnabled(true);[m
[32m+[m[32m        actionBar.setNavigationMode(ActionBar.NAVIGATION_MODE_STANDARD);[m
[32m+[m[32m        actionBar.setTitle(R.string.app_name);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private ActionBar getActionBar() {[m
[32m+[m[32m        return ((AppCompatActivity) getActivity()).getSupportActionBar();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Callbacks interface that all activities using this fragment must implement.[m
[32m+[m[32m     */[m
[32m+[m[32m    public static interface NavigationDrawerCallbacks {[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Called when an item in the navigation drawer is selected.[m
[32m+[m[32m         */[m
[32m+[m[32m        void onNavigationDrawerItemSelected(int position);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
